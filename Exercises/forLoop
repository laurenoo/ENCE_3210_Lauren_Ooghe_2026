#include <stdio.h>
#include <math.h>

#define ARRAY_SIZE 10

int myArray[ARRAY_SIZE] ={0};
int sum = 0;
float avg;


int main() {
    
    for (int i=0; i<ARRAY_SIZE; i++) {
        scanf("%d", &myArray[i]);
    }
    for (int i=0; i<ARRAY_SIZE; i++) {
    sum += myArray[i];
    
    }
    
    avg = (float)sum / ARRAY_SIZE;
    
        printf("Sum = %d\n", sum);
        printf("Average = %.2f\n", avg);
    
    return 0;
    
}

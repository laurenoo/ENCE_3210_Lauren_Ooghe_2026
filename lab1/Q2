// Lab 1 - Q2 Lauren Ooghe
// Find the cosine of integer degrees between 0 and 360.
// The results should be saved in an array

# include <math.h>

float cosTable[361]; // Create array with 361 indices

// Using setup because only runs once
void setup() {

  Serial.begin(9600);  // begin serial monitor
  
  // Loop over from 0 to 360 degrees 
  for (int i = 0; i <= 360; i++) { 
    float rad = i * PI/180.0; // convert degrees to radians for cos function
    cosTable[i] = cos(rad); // save each value into the array each iteration of loop
    Serial.print("Angle: "); 
    Serial.print(i); // Print each value (degree)
    Serial.print(" deg   Cosine = ");
    Serial.println(cosTable[i], 6); // print cosine of each degree, rounded to 6 decimals
  }

}

void loop() {

}

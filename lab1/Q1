
#include <stdio.h>
#define ARRAY_SIZE 512

int myArray[ARRAY_SIZE] = {};
int twiceSquare(int a){
    
    return 2 * a * a;
}
int main()
{
    int results[ARRAY_SIZE] = {};
    
    for (int i=0; i < ARRAY_SIZE; i++) {
        results[i] = twiceSquare(i);
        
    }
    
    for (int i=0; i < ARRAY_SIZE; i++) {
        printf("x = %d, 2*x^2 = %d\n", i, results[i]);
    }
    
    return 0;
}
